        <item name="android:textSize">65sp</item>
        <item name="fontFamily">@font/button_font1</item>
        <item name="android:textColor">@color/white</item>

    </style>
    <style name="PuzzleButtonStyle">
        <item name="background">@drawable/menu_background2</item>
        <item name="android:layout_width">78dp</item>
        <item name="android:layout_height">78dp</item>
